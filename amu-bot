//Get discord.js
const Discord = require('discord.js');

//Create client instance as bot
const botAmu = new Discord.Client();

//Set listener on 'ready'
botAmu.on('ready', () => {
  console.log('Amu ready!');
});

//Set listener on 'message'
botAmu.on('message', message => {
  if (message.content === '1v1') {
    message.reply('QuickScope');
  }
});

botAmu.login('MjczMzEyOTI3MTE1OTAyOTc2.C2huJQ.1pKUt4QoVFw3ZpSP2sAkWVICGu4');
